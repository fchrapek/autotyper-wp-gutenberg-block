{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "atpr/autotyper",
  "version": "0.1.0",
  "title": "Autotyper",
  "category": "text",
  "icon": "flag",
  "description": "Simple block for autotyping text animation",
  "attributes": {
    "message": {
      "type": "string"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "autotyper",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}